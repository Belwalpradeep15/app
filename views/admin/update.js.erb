try {
    <% if @error %>
        alert('<%= I18n.t("admin.users.update_dialog.error", :error => escape_javascript(@error.to_s)) %>');
        window.location.reload(true);
    <% else %>
        <% if @formId %>
            enableRow('<%= @formId %>');
        <% end %>
        <% if @dialogId %>
            fotech.gui.FotechDialog.clearBusyState('<%= @dialogId %>');
        <% end %>
    <% end %>
}
catch (ex) {
    console.log("update.js.erb: " + ex);
}




