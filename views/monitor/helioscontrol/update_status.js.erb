<% if @id %>
    <% if @error %>
        alert("<%= @error %>");
    <% else %>
        fotech.gui.FotechDialog.setBusyState('helios_status_controls');
        monitor.helios.getHeliosStatus(<%= @id %>);
    <% end %>
    enableRow("helios_row_<%= @id %>");
<% end %>



