<p>You are not able to view this fibre line until its display type has been set.</p>

<script type="text/javascript">
    var fibreLine = getFibreLineById(<%= @global_fibre_lines[0].id %>);
    if (fibreLine) {
      setViewStatusForFibre(fibreLine);
    }
    resetDisplayHandlers();
    fotech.gui.enableMenuItem(jsmenubar, "viewFibreLine", false);
</script>
