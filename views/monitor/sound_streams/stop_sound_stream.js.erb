    <% if @error %>
        alert("<%= @error %>");
    <% else %>
        fotech.gui.rootOpener().globalSoundStreamManager.getAllByAttribute('fibreLineId', <%=@fibre_line.id%>).each( function(stream){
            fotech.gui.rootOpener().globalSoundStreamManager.update(stream.id, 'isStreaming', false);
        });
    <% end %>


